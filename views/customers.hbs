<div class="container">
    <div class="row">
      <div class="col-10">
        <h1>List of customers</h1>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <table class="table">
          <thead>
            <tr>
              <th>ID</th>
              <th>Title</th>
              <th>First name</th>
              <th>Last name</th>
              <th>Email</th>
            </tr>
          </thead>
          <tbody id="rows">
          {{#each objJson}}
            <tr>  
              <th scope="row">{{ this.id }}</th>
              <td>{{ this.title}}</td>
              <td>{{ this.firstname }}</td>
              <td>{{ this.surname }}</td>
              <td>{{ this.email }}</td>
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
